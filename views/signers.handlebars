<!DOCTYPE HTML>
<img id="image9"src="/image9.png">

<div class="boxOfPeople">
    <h2 id="signersh2"> HERE ARE {{count}} PEOPLE WHO HAVE SIGNED THE PETITION: </h2>

<ul class="listOfPeople">
    {{#each signers}}
    {{#if homepage}}

        <li> <a href="{{homepage}}">{{firstname}} {{lastname}}<a/>,{{age}},<a href="/signers/{{city}}">{{city}}</a></li>
